{
  "name": "section-code-separator",
  "displayName": "Section Code Separator",
  "description": "Ajoute des tirets sur la droite des commentaires utilisés  pour la séparation de code en ne dépassant pas les 80 caractères de la ligne.",
  "author": "Julien Wetzel",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/julienwetzel/vscode-section-code-separator.git"
  },
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:rust"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/node": "20.2.5",
    "typescript": "^5.1.3"
  }
}